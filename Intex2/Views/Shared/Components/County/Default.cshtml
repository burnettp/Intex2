
@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Summary" asp-route-county="" asp-route-pageNum="1" class="btn btn-outline-secondary">Summary</a>

    <h4>Counties</h4>
    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Summary" asp-route-county="@t" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedCounty == t ? "btn-primary" : "btn-outline-secondary")">@t</a>
            
    }

</div>